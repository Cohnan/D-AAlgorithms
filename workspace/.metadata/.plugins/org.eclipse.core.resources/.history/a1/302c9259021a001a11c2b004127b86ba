    /*
    private static int ip(int l) {
    	if (inFirstL.containsKey(l)) {
    		return candidatos.size(); 
    	}
    	return inFirstL.get(l);
    }*/
    
    private static int A(int i) {
    	return lista[i].a;
    }
    
    private static int B(int i) {
    	return lista[i].b;
    }
    
    /*
