import java.util.Arrays;
import java.util.LinkedList;
import java.util.ListIterator;

public class UndersLinkLists {
	public static void main(String[] args) {
		LinkedList<Integer> lista = new LinkedList<>();
				
		lista.add(2);
		ListIterator<Integer> it = lista.listIterator();
		
		System.out.println(it.previousIndex());
		System.out.println(it.nextIndex());
		System.out.println(it.hasNext());
		System.out.println("Next " + it.next());
		System.out.println(it.previousIndex());
		System.out.println("Prev " + it.previous()); //Same as last next call!!!
		System.out.println(it.nextIndex());
		System.out.println(it.hasNext());
		//it.next();
		//System.out.println(it.previous());
		
		System.out.println(lista);
		
		
		
		int[] vacia = new int[0];
		int[] nueva = Arrays.copyOf(vacia, 1);
		nueva[0] = 3;
		System.out.println(nueva[0]);
		System.out.println(nueva.length);
		
	}
}
